FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     compressibleInterIsoFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         0.35;

deltaT          1e-7;

writeControl    adjustableRunTime;

writeInterval   1e-3;

purgeWrite      0;

writeFormat     ascii; //when extrudeMesh, need to be binary

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.75;
maxAlphaCo      0.5;
maxDeltaT       6.e-7;

functions{
limitT
{
    type limitFields;
    libs (fieldFunctionObjects);
    fields (T);
    limit both;
    min 100;
    max 500;
    log false;
    writeControl none;
}
}
// ************************************************************************* //
